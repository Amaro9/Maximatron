<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="Maximatron.App"
             xmlns:local="using:Maximatron"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  
    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="/Controls/NewBlocControl.axaml"></StyleInclude>
    </Application.Styles>
             
    <Application.Resources>
        <SolidColorBrush x:Key="DarkGrey">#323232</SolidColorBrush>
        <SolidColorBrush x:Key="Pink">#ee87f9</SolidColorBrush>

    </Application.Resources>
</Application>